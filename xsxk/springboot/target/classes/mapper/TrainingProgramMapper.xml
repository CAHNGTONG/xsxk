<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.TrainingProgramMapper">
    <select id="selectBySpecialityId" resultType="com.example.entity.TrainingProgram">
        SELECT * FROM training_program WHERE speciality_id = #{specialityId}
    </select>
</mapper>